@Service
public class CourseServiceImpl implements CourseService {

    @Autowired
    private CoursesDao coursesDao ;


    @Override
    @PagingQuery
    public List<Courses> queryCourseList(Courses courses) {
        return coursesDao.queryCourseList(courses);
    }
}
